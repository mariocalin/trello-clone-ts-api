    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.6",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "optionalDependencies": {
    "win-node-env": "^0.6.0"
  }
