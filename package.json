  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.14.0"
  },
  "main": "dist/app/start.js",
  "scripts": {
    "lint": "npx eslint src/**/*.ts",
    "test:unit": "NODE_ENV=test npx jest --config=jest.config.js --collect-coverage --setupFiles dotenv-flow/config",
    "start:dev": "NODE_ENV=development node dist/app/start.js",
    "build": "npm run build:clean && npm run build:tsc",
    "build:tsc": "npx tsc",
    "build:clean": "rimraf dist/*",
    "build:prod": "npm run build:clean && npm run build:tsc-prod",
    "build:tsc-prod": "npx tsc -p tsconfig.prod.json"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "dotenv-flow": "^3.2.0",
    "env-var": "^7.1.1",
    "express": "^4.17.2",
    "express-promise-router": "^4.1.1",
    "glob": "^7.2.0",
    "http-status": "^1.5.0",
    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.1.0",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@jest-mock/express": "^1.4.5",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.8",
